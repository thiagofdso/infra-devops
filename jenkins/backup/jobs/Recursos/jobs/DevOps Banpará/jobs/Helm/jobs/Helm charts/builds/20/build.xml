    stage(&apos;Construcao&apos;) {
      steps {
        updateGitlabCommitStatus name: &apos;2.2-Sonar-Construcao&apos;, state: &apos;running&apos;

		withCredentials([usernamePassword(credentialsId: &apos;gen_jenkins&apos;, passwordVariable: &apos;PASS&apos;, usernameVariable: &apos;USER&apos;)]) {

             script {
                  def count = sh(script: &apos;ls -l charts/ | grep -c ^d&apos;, returnStdout: true).trim()
                  echo &quot;numero de charts: ${count}&quot;
                  def pastas = sh(script:&apos;ls charts&apos;, returnStdout: true).split(&apos; &apos;)
                  for (pasta in pastas) {
                      pasta=pasta.trim()
                      env.CHART_VERSION = sh(script: &quot;cat charts/${pasta}/Chart.yaml | grep version | cut -d &apos; &apos; -f 2&quot;, returnStdout: true).trim()
                      echo &quot;------- publicando chart ${pasta}-${CHART_VERSION} -------&quot;
                      sh &quot;helm package -d . ./charts/${pasta}/&quot;
                      sh &quot;curl -v -u &apos;$USER:$PASS&apos; --upload-file ${pasta}-${CHART_VERSION}.tgz &apos;$NEXUS_REPO/charts/${pasta}-${CHART_VERSION}.tgz&apos;&quot;
                    }
                   echo &quot;------- Atualizando indice do repositorio -------&quot;
				    sh &quot;helm repo index . --url $NEXUS_REPO/charts&quot;
				    sh &apos;curl -v -u &quot;$USER:$PASS&quot; --upload-file index.yaml &quot;$NEXUS_REPO/index.yaml&quot;&apos;
              }

			updateGitlabCommitStatus name: &apos;2.2-Sonar-Construcao&apos;, state: &apos;success&apos;
		}
	  }
      post {
        failure {
          updateGitlabCommitStatus name: &apos;2.2-Analise Sonar&apos;, state: &apos;failed&apos;
        }
      }
    }
